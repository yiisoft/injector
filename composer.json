{
    "name": "yiisoft/injector",
    "type": "library",
    "description": "PSR-11 compatible injector. Executes a callable injecting dependencies from a given DI container.",
    "keywords": ["PSR-11", "DI", "dependency injection", "reflection", "injector"],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/injector/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/injector"
    },
    "require": {
        "php": "^7.2",
        "psr/container": "1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/factory": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Injector\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Injector\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
